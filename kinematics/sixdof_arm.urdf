<?xml version="1.0"?>
<robot name="six_dof_robotic_arm">

  <!-- Define joints -->
  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1.57079" upper="1.57079" effort="100" velocity="3.14"/>
  </joint>

  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0 0 14" rpy="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0" upper="3.66519" effort="100" velocity="3.14"/>
  </joint>

  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 13 0" rpy="0.785398 0 0"/>
    <axis xyz="-1 0 0"/>
    <limit lower="0" upper="3.14159" effort="100" velocity="3.14"/>
  </joint>


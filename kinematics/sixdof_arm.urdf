  <!-- Define materials -->
  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>

  <!-- Define links -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="9.0 9.0 2.9"/>
      </geometry>
      <origin xyz="0 0 1.45" rpy="0 0 0"/>
      <material name="black"/>
    </visual>
  </link>

  <link name="link1">
    <visual>
      <geometry>
        <box size="2.5 6.5 12.2"/>
      </geometry>
      <origin xyz="0 1.5 9.0" rpy="0 0 0"/> <!-- Origin height (z) is 6.1 (half of object height) + 2.9 (base height) = 9.0 -->
      <material name="black"/>
    </visual>
  </link>

  <link name="link2">
    <visual>
      <geometry>
        <box size="6 13 2.5"/>
      </geometry>
      <origin xyz="0 6.5 0" rpy="0 0 0"/>
